<h2>プロフィール編集</h2>
<%= form_with model: current_user do |f| %>
<% alert %>
  <h4>name</h4>
  <%= f.text_field :name %>
  <h4>introduction</h4>
  <%= f.text_field :introduction %>
  <label for="inputImage">ProfileImage</label>
  <%= f.file_field :image, placeholder: "プロフィール画像", accept: "image/*" %>  
  <%= f.submit '変更' %>
<% end %>